									</form>
									<form id="gapp-client-sendemail" role="form" action="/clients/sendemail" name="gapp-client-sendemail" method="post">
										<input type="hidden" id="client_domain" name="client_domain" />
									</form>
									<form id="gapp-client-home" role="form" action="/clienthome" name="gapp-client-home" method="post">
									</form>
								</div>
							</div>
						</div>
					</div>
